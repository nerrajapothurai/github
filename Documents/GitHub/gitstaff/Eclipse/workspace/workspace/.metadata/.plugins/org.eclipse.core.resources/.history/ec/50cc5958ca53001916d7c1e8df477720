package simple;

public class Array2d {
	public static void main(String[] args) {
		int[][] X=new int[2][3];
		X[0][0]=10;
		X[0][1]=20;
		X[0][2]=30;
		X[1][0]=40;
		X[1][1]=50;
		X[1][2]=60;
		System.out.println(X.length);
		for(int i=0;i<X.length;i++)
		{
			for(int j=0;j<X[i].length;j++)
			{
				System.out.println(X[i][j]);
			}
		}
	}

}
